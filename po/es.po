msgstr "No es capaz de abrir %s"

#, c-format
msgid "Unable to parse RNG %s"
msgstr "No es capaz de interpretar el RNG %s"

msgid "Unable to parse configuration"
msgstr "No es capaz de interpretar la configuración"

msgid "Unable to perform RecvAll"
msgstr "No es capaz de realizar RecvAll"

msgid "Unable to perform SendAll"
msgstr "No es capaz de realizar SendAll"

msgid "Unable to restore domain"
msgstr "No es capaz de restaurar el dominio"

msgid "Unable to validate doc"
msgstr "No es capaz de validar el documento"

